<Application
    x:Class="ModbusTesting.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:ModbusTesting"
    StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <local:MainTest x:Key="maintest" />
            <local:TextConverters x:Key="textConverter" />
            <ResourceDictionary.MergedDictionaries>
                <local:SharedResourceDictionary Source="pack://application:,,,/ModbusTesting;component/Brushes_Classic.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
